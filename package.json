{
  "name": "install-node",
  "version": "0.0.0",
  "description": "Install node into node_modules",
  "bin": {
    "install-node": "bin/run.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "preferGlobal": true,
  "scripts": {
    "test": "egg-bin test",
    "lint": "eslint . --ext js --fix",
    "ci": "npm run lint && egg-bin cov",
    "autod": "autod -w --prefix='^'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/node-modules/install-node.git"
  },
  "keywords": [
    "install-node"
  ],
  "author": "fengmk2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/node-modules/install-node/issues"
  },
  "homepage": "https://github.com/node-modules/install-node#readme",
  "engines": {
    "node": ">= 4.0.0"
  },
  "ci": {
    "version": "4, 6"
  },
  "dependencies": {
    "common-bin": "^2.0.0"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "egg-bin": "^1.3.0",
    "egg-ci": "^1.0.3",
    "eslint": "^3.3.1",
    "eslint-config-egg": "^3.1.0"
  }
